<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
<class name="hibernateobjects.NseWaveData" table="nse_wave">
 
<composite-id>
<key-property name="stocksymbol" column="stocksymbol"  />
<key-property name="duration" column="duration" />
</composite-id>

<property name="support" column="lowband"/>
<property name="resistance" column="highband"/>


</class>
</hibernate-mapping>